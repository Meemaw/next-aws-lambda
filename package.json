{
  "name": "next-aws-lambda",
  "version": "0.0.0-development",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "serverless --verbose",
    "test": "npm run test:unit",
    "test:unit": "jest",
    "test:e2e": "testcafe chrome e2e/specs",
    "test:e2e:headless": "testcafe chrome:headless e2e/specs",
    "lint": "concurrently \"npm run prettier\" \"npm run eslint\"",
    "eslint": "eslint --max-warnings 0 --ext .ts 'src/**/*' 'e2e/**/*'",
    "prettier": "prettier -l 'src/**/*'"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.6",
    "next": "^9.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@testing-library/testcafe": "^3.3.0",
    "@types/aws-serverless-express": "^3.3.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "concurrently": "^5.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "serverless": "^1.54.0",
    "serverless-next.js": "^1.4.0",
    "serverless-offline": "^5.12.0",
    "testcafe": "^1.5.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
