{
  "name": "next-aws-lambda",
  "version": "0.0.0-development",
  "description": "Minimal starter boilerplate project for Serverless Next.js on AWS Lambda",
  "license": "MIT",
  "author": "Meemaw <ematej.snuderl@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Meemaw/next-aws-lambda"
  },
  "bugs": {
    "url": "https://github.com/Meemaw/next-aws-lambda/issues"
  },
  "scripts": {
    "dev": "next dev",
    "dev:background": "npm run dev 2>&1 > /dev/null &",
    "build": "next build",
    "start": "next start",
    "deploy": "serverless --verbose --no-aws-s3-accelerate",
    "test": "npm run test:unit",
    "test:unit": "jest",
    "test:e2e": "testcafe chrome e2e/specs",
    "test:e2e:headless": "testcafe chrome:headless e2e/specs",
    "lint": "concurrently \"npm run prettier\" \"npm run eslint\"",
    "eslint": "eslint --max-warnings 0 'src/**/*.{ts,tsx}' 'e2e/**/*.{ts,tsx}'",
    "prettier": "prettier -l 'src/**/*' 'e2e/**/*'",
    "storybook": "start-storybook"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.6",
    "next": "^9.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@storybook/react": "^5.3.8",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "@testing-library/testcafe": "^3.3.1",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "@types/react": "^16.9.19",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "serverless": "^1.61.3",
    "serverless-next.js": "^1.8.0",
    "serverless-offline": "^5.12.1",
    "testcafe": "^1.8.1",
    "ts-jest": "^25.0.0",
    "wait-on": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
